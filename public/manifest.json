{
  "manifest_version": 3,
  "name": "LV Wallet",
  "version": "1.0",
  "description": "A simple extension to get started.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo1.png",
      "48": "logo1.png",
      "128": "logo1.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}